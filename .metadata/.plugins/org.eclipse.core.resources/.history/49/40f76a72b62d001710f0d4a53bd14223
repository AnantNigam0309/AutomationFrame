package utils;

import java.util.logging.Logger;

import generics.constant;

public class xmlReporter extends constant {

	private String scenario=null;
	private String result=null;
	
	public String getScenario() {
		return scenario;
	}

	public void setScenario(String scenario) {
		this.scenario = scenario;
	}

	public String getResult() {
		return result;
	}

	public void setResult(String result) {
		this.result = result;
	}

	xmlReporter(String scenario,String result){
		log=Logger.getLogger(xmlReporter.class.getSimpleName().toString());
		log.info("Initialized Object for class xmlReporter...");
		this.scenario=scenario;
		this.result=result;
	}
	
	
}
